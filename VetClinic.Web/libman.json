{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.css",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.js"
      ]
    },
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/lib/jquery/",
      "files": ["dist/jquery.min.js", "dist/jquery.js"]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": ["dist/jquery.validate.min.js", "dist/jquery.validate.js"]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.min.js",
        "dist/jquery.validate.unobtrusive.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.0",
      "destination": "wwwroot/lib/microsoft-signalr/",
      "files": ["dist/browser/signalr.min.js", "dist/browser/signalr.js"]
    }
  ]
}
